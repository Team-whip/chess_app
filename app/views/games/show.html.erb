<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1 class="text-center"><%= @game.name %></h1>

  <table class="chess-board">
  	<% 8.times do |row| %>
	  <tr>
      <% 8.times do |column| %>
	<td data-x  = <%= column + 1 %> data-y = <%= row + 1 %>, class="chess-square"></td>
      <% end %>
    </tr>
  <% end %>
</table>


</div>
